<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="N">
  <rule type="fa" cat="N">
   <lf start="0" span="1" word="4" lemma="4" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
   <lf start="1" span="1" word="men" lemma="man" pos="NNS" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="conj" cat="N\N">
   <lf start="2" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
   <rule type="rp" cat="N">
    <rule type="fa" cat="N">
     <lf start="3" span="1" word="3" lemma="3" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="4" span="1" word="guitars" lemma="guitar" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="drums" lemma="drum" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <lf start="3" span="1" word="blue" lemma="blue" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
    </rule>
   </rule>
   <rule type="conj" cat="S[dcl]\S[dcl]">
    <lf start="4" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="ba" cat="S[dcl]">
     <lf start="5" span="1" word="there" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
     <rule type="fa" cat="S[dcl]\NP[thr]">
      <lf start="6" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="7" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="8" span="1" word="microphone" lemma="microphone" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="fa" cat="S[dcl]\NP[thr]">
    <lf start="1" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
    <rule type="lex" cat="NP">
     <rule type="ba" cat="N">
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="blue" lemma="blue" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="shirts" lemma="shirt" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="conj" cat="N\N">
       <lf start="5" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="fa" cat="N">
        <lf start="6" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="7" span="1" word="red" lemma="red" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="8" span="1" word="shirt" lemma="shirt" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
