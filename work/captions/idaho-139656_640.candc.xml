<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="NP">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="flock" lemma="flock" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="fa" cat="NP\NP">
   <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
   <rule type="ba" cat="NP[nb]">
    <rule type="lex" cat="NP">
     <lf start="3" span="1" word="sheep" lemma="sheep" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="conj" cat="NP[nb]\NP[nb]">
     <rule type="conj" cat="NP[nb]\NP[nb]">
      <lf start="4" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="fa" cat="NP[nb]">
       <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="6" span="1" word="horse" lemma="horse" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="An" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <rule type="fa" cat="N">
    <lf start="1" span="1" word="orange" lemma="orange" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
    <lf start="2" span="1" word="sky" lemma="sky" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <rule type="conj" cat="NP\NP">
   <lf start="3" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
   <rule type="rp" cat="NP">
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="flock" lemma="flock" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="6" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="lex" cat="NP">
       <rule type="ba" cat="N">
        <lf start="7" span="1" word="sheep" lemma="sheep" pos="NN" chunk="I-NP" entity="O" cat="N" />
        <rule type="conj" cat="N\N">
         <lf start="8" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
         <lf start="9" span="1" word="ahorseman" lemma="ahorseman" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP[nb]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="dog" lemma="dog" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="conj" cat="NP[nb]\NP[nb]">
   <lf start="2" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
   <rule type="ba" cat="NP[nb]">
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="horse" lemma="horse" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="conj" cat="NP[nb]\NP[nb]">
     <rule type="conj" cat="NP[nb]\NP[nb]">
      <lf start="5" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="7" span="1" word="sheep" lemma="sheep" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
