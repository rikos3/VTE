<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="boy" lemma="boy" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="fa" cat="S[ng]\NP">
     <lf start="3" span="1" word="wearing" lemma="wear" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="hat" lemma="hat" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="NP[nb]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <rule type="fa" cat="N">
    <lf start="1" span="1" word="boy" lemma="boy" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
    <rule type="funny" cat="N">
     <lf start="2" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="4" span="1" word="dogs" lemma="dog" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="0" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="1" span="1" word="dog" lemma="dog" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <lf start="3" span="1" word="white" lemma="white" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
    </rule>
   </rule>
   <rule type="conj" cat="S[dcl]\S[dcl]">
    <lf start="4" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="5" span="1" word="1" lemma="1" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="6" span="1" word="dog" lemma="dog" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="7" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
      <lf start="8" span="1" word="brown" lemma="brown" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
