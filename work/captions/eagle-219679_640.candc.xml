<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="bald" lemma="bald" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="eagle" lemma="eagle" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="5" span="1" word="yellow" lemma="yellow" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="6" span="1" word="beak" lemma="beak" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="bald" lemma="bald" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="eagle" lemma="eagle" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="5" span="1" word="white" lemma="white" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="6" span="1" word="head" lemma="head" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="An" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <rule type="fa" cat="N">
    <lf start="1" span="1" word="American" lemma="american" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
    <lf start="2" span="1" word="flag" lemma="flag" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <rule type="fa" cat="NP\NP">
   <lf start="3" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="5" span="1" word="bold" lemma="bold" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="6" span="1" word="eagle" lemma="eagle" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="7" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="lex" cat="NP">
       <lf start="8" span="1" word="front" lemma="front" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="9" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="rp" cat="NP">
        <lf start="10" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
        <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
