<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="NP[nb]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="child" lemma="child" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="conj" cat="NP[nb]\NP[nb]">
   <rule type="conj" cat="NP[nb]\NP[nb]">
    <lf start="2" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="puddle" lemma="puddle" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP[nb]">
  <rule type="lex" cat="NP">
   <rule type="fa" cat="N">
    <lf start="0" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="green" lemma="green" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="boots" lemma="boot" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <rule type="conj" cat="NP[nb]\NP[nb]">
   <rule type="conj" cat="NP[nb]\NP[nb]">
    <lf start="3" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="fa" cat="NP[nb]">
     <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="5" span="1" word="yellow" lemma="yellow" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="6" span="1" word="jacket" lemma="jacket" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP">
  <rule type="lex" cat="NP">
   <lf start="0" span="1" word="Sand" lemma="Sand" pos="NNP" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="conj" cat="NP\NP">
   <lf start="1" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
   <rule type="rp" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="2" span="1" word="sea" lemma="sea" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <lf start="3" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
