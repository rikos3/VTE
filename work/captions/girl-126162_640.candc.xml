<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="girl" lemma="girl" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="fa" cat="S[ng]\NP">
     <lf start="3" span="1" word="eating" lemma="eat" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="5" span="1" word="piece" lemma="piece" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="6" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="lex" cat="NP">
        <lf start="7" span="1" word="chocolate" lemma="chocolate" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="lex" cat="S[X]/S[X]">
   <rule type="fa" cat="S[ng]\NP">
    <lf start="0" span="1" word="Wearing" lemma="wear" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="2" span="1" word="cap" lemma="cap" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <rule type="lp" cat="S[dcl]">
   <lf start="3" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="girl" lemma="girl" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="6" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
      <rule type="fa" cat="S[ng]\NP">
       <lf start="7" span="1" word="eating" lemma="eat" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
       <lf start="8" span="1" word="something" lemma="something" pos="DT" chunk="I-NP" entity="O" cat="NP" />
      </rule>
     </rule>
    </rule>
    <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="NP">
  <rule type="ba" cat="NP">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="girl" lemma="girl" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="NP\NP">
    <lf start="2" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="4" span="1" word="black" lemma="black" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="5" span="1" word="coat" lemma="coat" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="woman" lemma="woman" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="fa" cat="S[ng]\NP">
     <lf start="3" span="1" word="wearing" lemma="wear" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="5" span="1" word="black" lemma="black" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="6" span="1" word="cap" lemma="cap" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
