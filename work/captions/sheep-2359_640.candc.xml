<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="3" lemma="3" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="white" lemma="white" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="2" span="1" word="sheep" lemma="sheep" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="ba" cat="S[ng]\NP">
     <lf start="4" span="1" word="laying" lemma="lay" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="5" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="7" span="1" word="ground" lemma="ground" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="One" lemma="one" pos="CD" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="1" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="sheep" lemma="sheep" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="4" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="6" span="1" word="black" lemma="black" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="7" span="1" word="head" lemma="head" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP">
    <lf start="0" span="1" word="All" lemma="all" pos="PDT" chunk="I-NP" entity="O" cat="NP/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="1" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="2" span="1" word="sheep" lemma="sheep" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="lay" lemma="lay" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="4" span="1" word="against" lemma="against" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="6" span="1" word="wall" lemma="wall" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
