<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="nest" lemma="nest" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="3" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="tree" lemma="tree" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="tree" lemma="tree" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <lf start="3" span="1" word="green" lemma="green" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="stork" lemma="stork" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="3" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="nest" lemma="nest" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
