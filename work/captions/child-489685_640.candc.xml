<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="girl" lemma="girl" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="cat" lemma="cat" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="5" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="her" lemma="her" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="7" span="1" word="lap" lemma="lap" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="NP">
  <rule type="ba" cat="NP">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="girl" lemma="girl" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="NP\NP">
    <lf start="2" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="blonde" lemma="blonde" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="4" span="1" word="hair" lemma="hair" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="girl" lemma="girl" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="ba" cat="S[ng]\NP">
     <lf start="3" span="1" word="sitting" lemma="sit" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
     <rule type="conj" cat="(S[ng]\NP)\(S[ng]\NP)">
      <lf start="4" span="1" word="and" lemma="and" pos="CC" chunk="I-VP" entity="O" cat="conj" />
      <rule type="fa" cat="S[ng]\NP">
       <lf start="5" span="1" word="holding" lemma="hold" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="7" span="1" word="cat" lemma="cat" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
