<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="men" lemma="man" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="ba" cat="S[ng]\NP">
     <rule type="fa" cat="S[ng]\NP">
      <lf start="3" span="1" word="playing" lemma="play" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="4" span="1" word="music" lemma="music" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="5" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="7" span="1" word="street" lemma="street" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="blind" lemma="blind" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="man" lemma="man" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="fa" cat="S[ng]\NP">
     <lf start="4" span="1" word="playing" lemma="play" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="5" span="1" word="accordion" lemma="accordion" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="man" lemma="man" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="fa" cat="S[ng]\NP">
     <lf start="3" span="1" word="playing" lemma="play" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="4" span="1" word="violin" lemma="violin" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
