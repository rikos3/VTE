<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="businessman" lemma="businessman" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="fa" cat="S[ng]\NP">
     <lf start="3" span="1" word="wearing" lemma="wear" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="5" span="1" word="balck" lemma="balck" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="6" span="1" word="necktie" lemma="necktie" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="fa" cat="S[dcl]\NP[thr]">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="laptop" lemma="laptop" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="6" span="1" word="brown" lemma="brown" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="7" span="1" word="table" lemma="table" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="cup" lemma="cup" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <lf start="3" span="1" word="green" lemma="green" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
