<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="NP[nb]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="man" lemma="man" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="conj" cat="NP[nb]\NP[nb]">
   <rule type="conj" cat="NP[nb]\NP[nb]">
    <lf start="2" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="beach" lemma="beach" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP[nb]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="dog" lemma="dog" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="conj" cat="NP[nb]\NP[nb]">
   <rule type="conj" cat="NP[nb]\NP[nb]">
    <lf start="2" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="man" lemma="man" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP[nb]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="sea" lemma="sea" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="conj" cat="NP[nb]\NP[nb]">
   <rule type="conj" cat="NP[nb]\NP[nb]">
    <lf start="2" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="bird" lemma="bird" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
