<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="fa" cat="S[dcl]\NP[thr]">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="cat" lemma="cat" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="lex" cat="NP\NP">
      <rule type="fa" cat="S[ng]\NP">
       <lf start="4" span="1" word="touching" lemma="touch" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="6" span="1" word="book" lemma="book" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP[thr]">
     <lf start="1" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="no" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="pictures" lemma="picture" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="4" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="6" span="1" word="book" lemma="book" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="cat" lemma="cat" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="3" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="mat" lemma="mat" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="mat" lemma="mat" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="3" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="grass" lemma="grass" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="cat" lemma="cat" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="appears" lemma="appear" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[to]\NP)" />
    <rule type="fa" cat="S[to]\NP">
     <lf start="3" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
     <rule type="fa" cat="S[b]\NP">
      <lf start="4" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[ng]\NP)" />
      <rule type="fa" cat="S[ng]\NP">
       <lf start="5" span="1" word="reading" lemma="read" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="7" span="1" word="novel" lemma="novel" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="cat" lemma="cat" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="fa" cat="(S[dcl]\NP)/PP">
     <lf start="2" span="1" word="touches" lemma="touch" pos="VBZ" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/PP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="novel" lemma="novel" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="PP">
     <lf start="5" span="1" word="with" lemma="with" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="6" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="7" span="1" word="left" lemma="left" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="8" span="1" word="paw" lemma="paw" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
