<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="lipstick" lemma="lipstick" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="woman" lemma="woman" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="5" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <lf start="6" span="1" word="red" lemma="red" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="woman" lemma="woman" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="holds" lemma="hold" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP[nb]">
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="apple" lemma="apple" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="conj" cat="NP[nb]\NP[nb]">
      <lf start="5" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="7" span="1" word="pear" lemma="pear" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="apple" lemma="apple" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <lf start="3" span="1" word="red" lemma="red" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
