<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="An" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="owl" lemma="owl" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="yellow" lemma="yellow" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="4" span="1" word="eyes" lemma="eye" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="NP">
  <rule type="ba" cat="NP">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="An" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="owl" lemma="owl" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="NP\NP">
    <lf start="2" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="snow" lemma="snow" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP[nb]">
  <rule type="fa" cat="NP[nb]">
   <lf start="0" span="1" word="An" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
   <lf start="1" span="1" word="owl" lemma="owl" pos="NN" chunk="I-NP" entity="O" cat="N" />
  </rule>
  <rule type="conj" cat="NP[nb]\NP[nb]">
   <rule type="conj" cat="NP[nb]\NP[nb]">
    <lf start="2" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
    <rule type="fa" cat="NP[nb]">
     <lf start="3" span="1" word="its" lemma="its" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="4" span="1" word="prey" lemma="prey" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
